percent = 40
event = 60
percent2 = int(percent)

hello = 100-percent2
largest = 0
if percent2 < hello:
  for i in range(1, percent2):
    if percent2 % i == 0 and hello % i == 0:
      largest = i
else:
  for i in range(0, hello):
    if percent2 % i == 0 and hello % i == 0:
      largest = i
percentfrac= percent2 / largest
hellofrac= hello/largest
print(percentfrac)
print(hellofrac)

